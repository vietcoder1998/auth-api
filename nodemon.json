{
  "watch": [
    "src",
    ".env",
    ".env.local",
    ".env.development",
    ".env.production",
    ".env.test"
  ],
  "ext": "ts,js,json,env",
  "ignore": [
    "node_modules",
    "dist",
    "coverage",
    "*.test.ts",
    "*.spec.ts"
  ],
  "exec": "ts-node src/index.ts",
  "env": {
    "NODE_ENV": "development"
  },
  "delay": 1000,
  "verbose": true,
  "restartable": "rs"
}