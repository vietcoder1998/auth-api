{
  "name": "auth-api",
  "version": "1.0.0",
  "description": "Authentication API with Node.js, Express, TypeScript, Prisma, Redis, Docker, Husky, Prettier, ESLint, Jest, and path alias.",
  "main": "src/index.ts",
  "scripts": {
    "dev": "npm-run-all --parallel dev:api gui:dev",
    "dev:api": "nodemon src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc",
    "test": "jest --config jest.config.js --coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "prepare": "husky install",
    "gui:dev": "npx serve src/gui -l 4000"
  },
  "dependencies": {
    "@prisma/client": "^4.16.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "prisma": "^4.16.2",
    "redis": "^4.6.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.0",
    "eslint": "^8.44.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "module-alias": "^2.2.3",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
